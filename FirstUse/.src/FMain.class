Public Sub Form_Open()
  Me.X = Screen.W / 2 - Me.W / 2
  Me.Y = Screen.H / 2 - Me.H / 2
End

Public Sub Button3_Click()
  Dim result As Integer
  Message.Title = "Cancel?"
  result = Message.Question("Do you wan't to camcel? ", " Yes ", " No ")
  If result = 1 Then
    Me.Close()
  Endif
End

Public Sub Button1_Click()

  Me.Hide()
  FMSoftware.Show()

End
